{
    "name": "ntnu-notifier",
    "version": "0.3.1",
    "description": "National Taiwan Normal University (NTNU) Notifier.",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib"
    ],
    "scripts": {
        "start": "ts-node -- ./src/",
        "test": "jest --coverage",
        "prepare": "npm run build",
        "dev": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/",
        "build": "npm run format && tsc",
        "doc": "typedoc ./src/",
        "format": "prettier --write \"src/**/*.ts\""
    },
    "keywords": [
        "NTNU",
        "National Taiwan Normal University"
    ],
    "author": "JacobLinCool <jacoblincool@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "cheerio": "^1.0.0-rc.10",
        "node-fetch": "^2.6.5"
    },
    "devDependencies": {
        "@types/jest": "^27.0.2",
        "@types/node": "^16.10.9",
        "@types/node-fetch": "^2.5.12",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "jest": "^27.2.5",
        "nodemon": "^2.0.13",
        "prettier": "^2.4.1",
        "ts-jest": "^27.0.7",
        "ts-node": "^10.3.0",
        "typedoc": "^0.22.5",
        "typescript": "^4.4.4"
    },
    "bugs": {
      "url": "https://github.com/JacobLinCool/NTNU-Notifier/issues"
    },
    "homepage": "https://github.com/JacobLinCool/NTNU-Notifier#readme",
